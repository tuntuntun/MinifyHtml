{
    "name": "wyrihaximus/minify-html",
    "description": "Html minifier for CakePHP3 and CakePHP4/5",
    "type": "cakephp-plugin",
    "license": "MIT",
    "keywords": [
        "html",
        "compress",
        "minify",
        "cakephp",
        "cakephp4",
        "cakephp5"
    ],
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com",
            "homepage": "http://wyrihaximus.net/"
        }
    ],
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "^4.0 || ^5.0",
        "wyrihaximus/html-compress": "^2.0 || ^3.0 || ^4.0",
        "cakephp/chronos": "^3.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5 || ^10.0",
        "phpspec/prophecy": "^1.15",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\MinifyHtml\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\CakePHP\\Tests\\MinifyHtml\\View\\Helper\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
